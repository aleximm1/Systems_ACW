<Window x:Class="Systems_ACW.LogInWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Systems_ACW"
        mc:Ignorable="d"
        Title="LogInWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="StudentIdTextbox" HorizontalAlignment="Left" Height="23" Margin="254,88,0,0" TextWrapping="Wrap" Text="Student ID" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="PasswordTextbox" HorizontalAlignment="Left" Height="23" Margin="254,129,0,0" TextWrapping="Wrap" Text="Password" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="LoginButton" Content="Log In" HorizontalAlignment="Left" Margin="276,182,0,0" VerticalAlignment="Top" Width="75" Click="LoginButton_Click"/>
        <Button x:Name="RegisterButton" Content="Button" HorizontalAlignment="Left" Margin="190,182,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
